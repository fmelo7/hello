{
  "app": {
    "name": "hello",
    "baseRoute": "/hello",
    "port": 8080
  },
  "log": {
    "enabled": true
  },
  "db": {
    "url": "mongodb://localhost:27017/hello",
    "options": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true,
      "poolSize": 10
    }
  },
  "cotacaoDolarService": {
    "url": "https://olinda.bcb.gov.br/olinda/servico/PTAX/versao/v1/odata/CotacaoDolarDia(dataCotacao=@dataCotacao)",
    "timeout": 12000
  }
}